T_1.9 ;
    %jmp T_1.7;
T_1.6 ;
    %load/vec4 v0x55ffcf0d11f0_0;
    %load/vec4 v0x55ffcf0d1000_0;
    %pad/u 6;
    %sub;
    %cmpi/u 0, 0, 6;
    %flag_or 5, 4;
    %jmp/0xz  T_1.10, 5;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x55ffcf0d11f0_0, 0;
    %jmp T_1.11;
T_1.10 ;

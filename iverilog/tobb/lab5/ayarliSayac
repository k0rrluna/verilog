    %load/vec4 v0x55ffcf0d1000_0;
    %pad/u 6;
    %sub;
    %assign/vec4 v0x55ffcf0d11f0_0, 0;
T_1.11 ;
T_1.7 ;
T_1.4 ;
T_1.2 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55ffcf0ba4e0;
T_2 ;
    %load/vec4 v0x55ffcf0d1530_0;
    %inv;
    %store/vec4 v0x55ffcf0d1530_0, 0, 1;
    %delay 1, 0;
